{
  "expo": {
    "name": "Stylish",
    "slug": "stylish",
    "scheme": "stylish",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/app.png", 
    "splash": {
      "image": "./assets/app.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFAFA"
    },
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.mostofafatin.stylish",
      "supportsTablet": true
    },
    "android": {
      "package": "com.mostofafatin.stylish",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app.png",
        "backgroundColor": "#FAFAFA"
      },
      "edgeToEdgeEnabled": true,
      "jsEngine": "hermes"
    },
    "web": {
      "favicon": "./assets/small.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "84cb9fd6-58b1-4ac3-8939-f1ed107c54dd"
      }
    }
  }
}
