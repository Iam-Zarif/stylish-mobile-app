{
  "expo": {
    "name": "Stylish",
    "slug": "my-app",
    "scheme": "your-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/app.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app.png"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/small.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
